{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "16.13.1"
        }
      }
    ],
    "@babel/preset-typescript",
    "minify"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@constants": "./src/constants",
          "@tests": "./src/tests"
        }
      }
    ]
  ],
  "ignore": ["./src/tests"]
}
